{
  "name": "Custom AdGuard Filter",
  "description": "Compiled filter for AdGuard Home",
  "homepage": "https://example.org",
  "license": "GPLv3",
  "version": "1.0.0",
  "sources": [
    {
      "name": "Local rules",
      "source": "rules.txt",
      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress"
      ],
      "exclusions_sources": [
        "exclusions.txt"
      ]
    },
    {
      "name": "ppfeufer list",
      "source": "https://github.com/ppfeufer/adguard-filter-list/blob/master/blocklist?raw=true",
      "type": "adblock"
    }
  ],
  "transformations": [
    "Deduplicate",
    "Compress"
  ],
  "exclusions_sources": [
    "global_exclusions.txt"
  ]
}